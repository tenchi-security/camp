{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectVersion",
                "s3:ListBucket",
                "s3:ListBucketVersions",
                "s3:PutObject",
                "s3:GetBucketLocation",
                "kms:Decrypt",
                "kms:GenerateDataKey",
                "sqs:ReceiveMessage",
                "sqs:ChangeMessageVisibility",
                "sqs:DeleteMessage",
                "sqs:GetQueueUrl",
                "sqs:SendMessage",
                "sqs:GetQueueAttributes",
                "sqs:ListQueues"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Deny",
            "NotAction": [
                "s3:GetObject",
                "s3:GetObjectVersion",
                "s3:ListBucket",
                "s3:ListBucketVersions",
                "s3:PutObject",
                "s3:GetBucketLocation",
                "kms:Decrypt",
                "kms:GenerateDataKey",
                "sqs:ReceiveMessage",
                "sqs:ChangeMessageVisibility",
                "sqs:DeleteMessage",
                "sqs:GetQueueUrl",
                "sqs:SendMessage",
                "sqs:GetQueueAttributes",
                "sqs:ListQueues"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Deny",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectVersion",
                "s3:ListBucket",
                "s3:ListBucketVersions",
                "s3:PutObject",
                "s3:GetBucketLocation"
            ],
            "NotResource": [
                "arn:aws:s3:::aws-security-data-lake*"
            ]
        },
        {
            "Effect": "Deny",
            "Action": [
                "sqs:ReceiveMessage",
                "sqs:ChangeMessageVisibility",
                "sqs:DeleteMessage",
                "sqs:GetQueueUrl",
                "sqs:SendMessage",
                "sqs:GetQueueAttributes",
                "sqs:ListQueues"
            ],
            "NotResource": "arn:aws:sqs:*:*:AmazonSecurityLake*"
        },
        {
            "Effect": "Deny",
            "Action": [
                "kms:Decrypt",
                "kms:GenerateDataKey"
            ],
            "Resource": "*",
            "Condition": {
                "StringNotLike": {
                    "kms:ViaService": [
                        "s3.*.amazonaws.com",
                        "sqs.*.amazonaws.com"
                    ]
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": [
                "kms:Decrypt",
                "kms:GenerateDataKey"
            ],
            "Resource": "*",
            "Condition": {
                "Null": {
                    "kms:EncryptionContext:aws:s3:arn": "false"
                },
                "StringNotLikeIfExists": {
                    "kms:EncryptionContext:aws:s3:arn": [
                        "arn:aws:s3:::aws-security-data-lake*"
                    ]
                }
            }
        },
        {
            "Effect": "Deny",
            "Action": [
                "kms:Decrypt",
                "kms:GenerateDataKey"
            ],
            "Resource": "*",
            "Condition": {
                "Null": {
                    "kms:EncryptionContext:aws:sqs:arn": "false"
                },
                "StringNotLikeIfExists": {
                    "kms:EncryptionContext:aws:sqs:arn": [
                        "arn:aws:sqs:*:*:AmazonSecurityLake*"
                    ]
                }
            }
        }
    ]
}