{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "iq:*",
                "iq-permission:*"
            ],
            "Effect": "Allow",
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": "iam:CreateServiceLinkedRole",
            "Resource": "*",
            "Condition": {
                "StringEquals": {
                    "iam:AWSServiceName": [
                        "permission.iq.amazonaws.com",
                        "contract.iq.amazonaws.com"
                    ]
                }
            }
        }
    ]
}